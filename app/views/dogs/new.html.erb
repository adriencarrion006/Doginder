     <h3>Create a new dog</h3>
        <%= simple_form_for @dog do |f| %>
          <%= f.input :name %>
          <%= f.input :breed %>
          <%= f.input :description %>
          <%= f.input :photos, as: :file, input_html: { multiple: true } %>
          <%= f.input :location_cp %>
          <%= f.submit class: 'btn btn-flat' %>
        <% end %>
